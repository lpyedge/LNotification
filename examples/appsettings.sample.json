{
  "LNotification": {
    "MaxRetries": 3,
    "RetryDelayMs": 1000,
    "TimeoutSeconds": 30,
    "Providers": [
      {
        "Provider": "Slack",
        "Alias": "default",
        "WebhookUrl": "https://example.com/slack-webhook"
      },
      {
        "Provider": "Telegram",
        "Alias": "alerts",
        "BotToken": "bot-token",
        "ChatId": "123456"
      },
      {
        "Provider": "Discord",
        "Alias": "devops",
        "WebhookUrl": "https://example.com/discord-webhook"
      },
      {
        "Provider": "Teams",
        "Alias": "teams",
        "WebhookUrl": "https://example.com/teams-webhook"
      },
      {
        "Provider": "Feishu",
        "Alias": "feishu",
        "WebhookUrl": "https://example.com/feishu-webhook"
      },
      {
        "Provider": "Email",
        "Alias": "smtp",
        "SmtpHost": "smtp.example.com",
        "SmtpPort": 587,
        "EnableSsl": true,
        "FromAddress": "noreply@example.com",
        "FromDisplayName": "Notifier",
        "Username": "smtp-user",
        "Password": "smtp-pass",
        "To": [
          "ops@example.com"
        ],
        "Cc": [
          "dev@example.com"
        ],
        "Bcc": [
          "audit@example.com"
        ],
        "SubjectPrefix": "[Notify]",
        "IsHtml": true
      }
    ]
  }
}
